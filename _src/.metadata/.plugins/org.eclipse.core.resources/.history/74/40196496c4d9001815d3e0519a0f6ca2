 * 
 */
package com.test.privateCar;

/**
 * @author Nilesh
 *
 */
 public class PrivateCar {

		public String color;
		private String type;
		
		public PrivateCar(String color,String type){
			this.color = color;
			this.type = type;
		}
		
		void fill(String fuel){
			System.out.println("fueling with "+fuel);
			this.type = fuel;
		}
		
		void empty() {
			this.type = "";
		}
}
